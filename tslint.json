{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "jsx-no-lambda":true,
      "strict-type-predicates": true,
      "ordered-imports": true,
      "member-ordering": true,
      "jsx-boolean-value": true

    }
  }